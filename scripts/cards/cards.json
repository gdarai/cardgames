[
  {
    "_process":"EXPORT_TABLE",
    "_sourceOds":"cards.ods",
    "target":"csv_A.csv",
    "sheet":"Test A",
    "skipTitle":1,
    "table":1
  },
  {
    "_process":"EXPORT_TABLE",
    "_sourceOds":"cards.ods",
    "target":"csv_comb.csv",
    "sheet":"Test COMB",
    "skipTitle":1,
    "table":1
  },
  {
    "_process":"CONVERT_SVGS"
  },
  {
    "_process":"COMBINE_PNGS",
    "_formula":["Ax", "Bx", "Cx"],
    "_skipMissing":"True",
    "_align":"center",
    "_shape":"row",
    "_skip":[ "" ],
    "_out":{ "reg": "png/comb_{colTgt}.png" },
    "Ax":{ "reg": "png/{colA}.png" },
    "Bx":{ "reg": "?{colB}{}png/{colB}.png" },
    "Cx":{ "reg": "?{colC}{}png/{colC}.png" },
    "_list":{
      "values":[
        "colA",
        "colB",
        "colC",
        "colTgt"
      ]
    },
    "values":"csv_comb.csv"
  },
  {
    "_process":"PRINT_CARDS",
    "_out":"_OUT_A.tex",
    "_card":"png/card_100x150",
    "_onOneLine":3,
    "_randomize":"True",
    "_list":{
      "values":[
        "name",
        "text",
        "speed",
        "img"
      ]
    },
    "values":"csv_A.csv"
  }
]
